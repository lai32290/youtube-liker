{
  "name": "Youtube Liker",
  "version": "1.0.2",
  "description": "A easy way to reminder you to like the Youtube video",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/watch*",
        "http://*.youtube.com/watch*"
      ],
      "js": [ "inject.js" ]
    }
  ]
}
